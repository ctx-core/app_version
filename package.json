{
	"name": "@ctx-core/app_version",
	"version": "11.0.23",
	"description": "ctx-core app_version",
	"keywords": [
		"ctx-core",
		"app_version"
	],
	"homepage": "https://github.com/ctx-core/app_version#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/app_version/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/app_version/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^22.0.16",
		"@ctx-core/atob": "workspace:^9.0.11",
		"@ctx-core/env": "workspace:^12.0.21",
		"@ctx-core/object": "workspace:^20.2.2",
		"@ctx-core/store": "workspace:^26.0.17",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^6.0.15",
		"@types/node": "^15.12.5",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
