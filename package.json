{
	"name": "@ctx-core/app_version",
	"version": "16.0.1",
	"description": "ctx-core app_version",
	"keywords": [
		"ctx-core",
		"app_version"
	],
	"homepage": "https://github.com/ctx-core/app_version#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/app_version/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/app_version.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^24.0.1",
		"@ctx-core/atob": "workspace:^10.0.46",
		"@ctx-core/env": "workspace:^16.0.1",
		"@ctx-core/nanostores": "workspace:^0.5.2",
		"@ctx-core/object": "workspace:^22.0.13",
		"svelte": "^3.46.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.135",
		"@types/atob-lite": "^2.0.0",
		"@types/node": "^17.0.14",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js",
	"sideEffects": false
}