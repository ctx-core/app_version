		"ctx-core",
		"app_version"
	],
	"homepage": "https://github.com/ctx-core/app_version#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/app_version/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/app_version/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^21.3.0",
		"@ctx-core/atob": "workspace:^8.0.2",
		"@ctx-core/env": "workspace:^11.0.17",
		"@ctx-core/object": "workspace:^19.1.5",
		"@ctx-core/store": "workspace:^25.1.17",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^5.1.4",
		"@types/node": "^15.12.2",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}