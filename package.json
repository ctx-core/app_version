	"description": "ctx-core app_version",
	"keywords": [
		"ctx-core",
		"app_version"
	],
	"homepage": "https://github.com/ctx-core/app_version#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/app_version/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/app_version/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^22.0.3",
		"@ctx-core/atob": "workspace:^9.0.1",
		"@ctx-core/env": "workspace:^12.0.5",
		"@ctx-core/object": "workspace:^20.0.2",
		"@ctx-core/store": "workspace:^26.0.3",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^6.0.2",
		"@types/node": "^15.12.2",
		"typescript": "^4.3.3"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}